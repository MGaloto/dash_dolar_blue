1694351021463:return(
1694351021472:c("danger", "arrow-up")
1694351021483:)
1694351021494:} else if (actual_value < before_value) {
1694351021504:return(
1694351021514:c("primary", "arrow-down")
1694351021526:)
1694351021537:} else {
1694351021549:c("teal","equals")
