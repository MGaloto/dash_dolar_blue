0:hc_xAxis(categories = bluevsmep$Fecha) %>%
0:hc_yAxis(title = list(text = "Brecha")) %>%
0:hc_add_series(name = "brecha",
0:data = bluevsmep$brecha,
0:color = "#007bff") %>%
0:highcharter::hc_tooltip(crosshairs = TRUE, pointFormat = "Brecha: % {point.y}") %>%
